<div id="{{ id }}" {{ attributes.addClass(classes).removeClass('panel-group') }}>
  {% for key, row in rows -%}
    <div class="panel panel-default mrgn-tp-md">
      <div class="panel-body">
        <h4 class="panel-title">
          {{row.title}}
        </h4>
        {{row.content}}
      </div>
    </div>
  {%- endfor %}
</div>
